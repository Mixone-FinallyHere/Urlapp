<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="be.urlab.Urlapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="be.urlab.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/custom">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Pamela.PamelaActivity" />
        <activity
            android:name=".Calendar.CalendarActivity"
            android:label="@string/title_activity_agenda__side__menu"
            android:parentActivityName="be.urlab.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="be.urlab.MainActivity" />
        </activity>
    </application>

</manifest>
